<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <TableLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/tableLayout1">
        <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content">
        
        <TextView
         	android:id="@+id/d_tv"
        	android:layout_width="wrap_content"
			android:layout_height="wrap_content" 
			android:textSize="16dp"
			/>
        </TableRow>
        <TableRow 
        android:paddingTop="40px"
        android:gravity="center_horizontal"
        android:id="@+id/tableRow2" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <Button android:text="Back to Video List" android:id="@+id/button1" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
        
        
        </TableRow>
    </TableLayout>
    
</LinearLayout>
